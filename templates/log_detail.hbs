<div class="log" id="log-{{title}}">

	<a href="#log" class="back">back</a>
	<div class="header">
		<div class="title">{{detail}}</div>
		<div class="blurb">{{date}}</div>
	</div>
	<div class="log-contents">
		{{{logContents}}}
		<div class="canvas-wrapper">
			<canvas id="fft"></canvas>
			<canvas id="letters" ></canvas>
		</div>
		<div id="audio"></div>
		<div id="controls">
			<div id="toggler"></div>
			<div id="time">
				<div class="elapsed">0</div>
				/
				<div class="total"></div>
			</div>
		</div>
	</div>
</div>
