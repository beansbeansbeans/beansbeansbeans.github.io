<div class="log" id="log-{{title}}">
	<a href="#log" class="back">back</a>
	<div class="log-contents">
		{{{logContents}}}
		<div class="canvas-wrapper">
			<canvas id="fft"></canvas>
			<canvas id="letters" ></canvas>
		</div>
		<div class="header">
			<div class="slider">
				<div class="label">Guilt: </div>
				<div class="bar">
					<div style="left: 20%;" class="highlight"></div>
				</div>
			</div>
		</div>
		<div id="audio"></div>
		<div id="controls">
			{{#if previous}}
				<a href="#log/{{previous}}" id="previous">previous</a>
			{{/if}}
			{{#if next}}
				<a href="#log/{{next}}" id="next">next</a>
			{{/if}}
			<div id="toggler"></div>
			<div id="time">
				<div class="elapsed">0</div>
				/
				<div class="total"></div>
			</div>
		</div>
	</div>
</div>
