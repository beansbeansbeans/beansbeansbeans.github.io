<nav id="project_index">
	{{#each this}}
	    <li data-project-id={{title}}>
	    	<a href="#projects/{{title}}">
	    		<div style="background-image:url(../images/project_index/{{title}}.jpg)" class="image"></div>
	    		<div class="title">
	    			{{title}}
	    		</div>
	    		<div class="description">
	    			{{description}}
	    		</div>
    		</a>
	    </li>
    {{/each}}
</nav>